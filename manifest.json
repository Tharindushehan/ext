{
    "name": "NOFISH Extention CDAP",
    "version": "1.0",
    "description": "This works hard to prevent you from being fished",
    "options_page":"option.html",
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icon.png"
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2
}